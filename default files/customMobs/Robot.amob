  body:
    forwardBackward: 0.3
    upDown: -0.95
    rightLeft: 0.0
    visible: false
    mini: false
    headPose: 0;0;270
    headItem: skin 

# Mobs Animations
# Explanation: https://as.advancedplugins.net/animations
animations:
  1:
    body:
      headPose: 0;180;270
      returnToOriginaPose: false
  1:
    body:
      headPose: 0;360;270
      returnToOriginaPose: false